   "source": [
    "from typing import List\n",
    "\n",
    "def twoSum(nums: List[int], target: int):\n",
    "    seen = {}\n",
    "\n",
    "    for i, num in enumerate(nums):\n",
    "        complement = target - num\n",
    "        if complement in seen:\n",
    "            return (seen[num], i)\n",
    "        seen[num] = i\n",
    "    return None"
   ]
